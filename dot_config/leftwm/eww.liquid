[
{% for tag in workspace.tags %}
{% if tag.mine %}
{
    "class": "ws-button-mine",
    "click": "leftwm command 'SendWorkspaceToTag {{workspace.index}} {{tag.index}}'",
    "icon": ""
}
{% elsif tag.visible  %}
{
    "class": "ws-button-visible",
    "click": "leftwm command 'SendWorkspaceToTag {{workspace.index}} {{tag.index}}'",
    "icon": ""
}
{% elsif tag.busy  %}
{
    "class": "ws-button-busy",
    "click": "leftwm command 'SendWorkspaceToTag {{workspace.index}} {{tag.index}}'",
    "icon": ""
}
{% else tag.visible  %}
{
    "class": "ws-button",
    "click": "leftwm command 'SendWorkspaceToTag {{workspace.index}} {{tag.index}}'",
    "icon": ""
}
{% endif %}
{% if forloop.last %}
{% else %}
,
{% endif %}
{% endfor %}
]
